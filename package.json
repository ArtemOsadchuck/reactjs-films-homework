{
    "name": "react-homework",
    "version": "1.0.0",
    "description": "",
    "main": "src/index.jsx",
    "scripts": {
        "lint:fix": "eslint . --ext .tsx, --fix",
        "lint": "eslint . --ext .tsx,.ts && stylelint ./src/**/*.{css,scss,sass}",
        "stylelint:fix": "stylelint ./src/**/*.{css,scss,sass} --fix",
        "test": "cross-env NODE_ENV=test jest",
        "prepare": "husky install",
        "develop": "cross-env NODE_ENV=development webpack --config config/webpack/webpack.config.js",
        "prod": "cross-env NODE_ENV=production webpack --config config/webpack/webpack.config.js && cross-env NODE_ENV=production node ./config/webpack/server.js",
        "start": "cross-env NODE_ENV=developmentServer node ./config/webpack/server.js",
        "build": "cross-env NODE_ENV=production webpack --config config/webpack/webpack.config.js"
    },
    "jest": {
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|JPG)$": "babel-jest",
            "^.+\\.(css|scss)$": "babel-jest"
        },
        "testPathIgnorePatterns": [
            "<rootDir>/node_modules/",
            "<rootDir>/legacy_notes_and_workbook/",
            "<rootDir>/client/src/stylesheets/"
        ]
    },
    "author": "artemOsd",
    "license": "ISC",
